<!-- Create your form here -->
<div>
  Student Form:

</div>

<%= form_tag students_path do %>
  <%= label_tag :'student[first_name]', "Student's First Name: "%>
  <%= text_field_tag(
      :'student[first_name]',
      nil,
      id: "first_name"
  )
  %><br>

  <%= label_tag :'student[last_name]', "Student's Last Name: "%>
  <%= text_field_tag(
      :'student[last_name]',
      nil,
      id: "last_name"
  )
  %><br>

  <%= submit_tag "Submit Student" %>

<% end %>
